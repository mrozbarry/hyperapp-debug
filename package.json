{
  "name": "hyperapp-debug",
  "version": "2.0.0-beta.5",
  "source": "./src/lib/index.js",
  "main": "hyperapp-debug.js",
  "module": "hyperapp-debug.module.js",
  "unpkg": "hyperapp-debug.umd.js",
  "license": "MIT",
  "repository": "github:mrozbarry/hyperapp-debug",
  "scripts": {
    "start": "parcel serve ./src/test-bed/index.html",
    "build": "yarn build:library && yarn build:adapters",
    "build:library": "microbundle ./src/lib/hyperappDebug.js",
    "build:adapters": "",
    "prepack": "yarn build",
    "test": "ava",
    "test:coverage": "nyc ava"
  },
  "devDependencies": {
    "autoprefixer": "latest",
    "ava": "^3.15",
    "concurrently": "^4.1.2",
    "eslint": "^6.4.0",
    "esm": "^3.2.25",
    "hyperapp": "^2.0.18",
    "microbundle": "0.12.0-next.6",
    "nyc": "^15.1",
    "parcel-bundler": "^1.12.5",
    "sinon": "^11.1.2",
    "tailwindcss": "latest"
  },
  "peerDependencies": {
    "hyperapp": "^2"
  },
  "ava": {
    "files": [
      "src/**/*.test.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "failFast": true,
    "failWithoutassertions": false,
    "require": [
      "esm"
    ]
  }
}
